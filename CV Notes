[Collection(nameof(ObjectCollection))]
public class PrimaryPhotoCoreChangeValueValidatorTests
{
    private readonly PrimaryPhotoCoreChangeValueArgsValidator validator;

    public PrimaryPhotoCoreChangeValueValidatorTests()
    {
        validator = new PrimaryPhotoCoreChangeValueArgsValidator();
    }

    [Fact]
    public async void Validate_should_return_expected_error_message_for_int_param()
    {
        //Arrange
        var args = new ChangeValueArgs();
        args.Param = "CountBoxes";
        args.Value = 10.25; 
        var expectedErrorMessage = $"Параметр '{args.Param}' должен быть целым числом или не задан.";

        //Act
        var result = await validator.TestValidateAsync(args);

        //Assert
        result.ShouldHaveValidationErrorFor(c => c.Value)
        .WithErrorMessage(expectedErrorMessage);
    }
}
