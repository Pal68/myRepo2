   public async Task ValidateAsync_should_throw_for_CoreFootage_greater_CoreOut()
    {
        //Arrange
        var gouge = factory.Fixture.Create<ObjGougeEntity>();
        var task = factory.Fixture.Create<PrcsTaskEntity>();
        task.Object = gouge.Object;

        this.limsDbContext.PrcsTasks.Add(task);
        await this.limsDbContext.SaveChangesAsync(CancellationToken.None);

        var notValidModel = new PrimaryPhotoCoreVariableModel{ CoreFootage = gouge.CoreOut + 10 };

        result.VariableModel = notValidModel;

        //Act, Assert
        await Assert.ThrowsAnyAsync<ValidationException>(async () => await result.ValidateAsync(task.Id, CancellationToken.None));
    }
